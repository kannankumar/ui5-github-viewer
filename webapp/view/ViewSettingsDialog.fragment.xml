<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog id="viewSettingsDialog" confirm="onConfirmViewSettingsDialog">
		<sortItems>
			<ViewSettingsItem text="{i18n>fullname}" key="full_name" selected="true"/>
			<ViewSettingsItem text="{i18n>updated}" key="updated_at"/>
			<ViewSettingsItem text="{i18n>createdAt}" key="created_at"/>
			<ViewSettingsItem text="{i18n>pushedAt}" key="pushed_at"/>
		</sortItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>